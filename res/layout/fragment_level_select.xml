<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:vowl="http://schemas.android.com/apk/res/edu.cmu.rwsefe.vowl"
    android:id="@+id/levelTable"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:stretchColumns="*"
    tools:context="edu.cmu.rwsefe.vowl.MainActivity$PlaceholderFragment" >

	<TableRow
	    android:layout_width="match_parent"
	    android:layout_height="match_parent" >
        
	    <edu.cmu.rwsefe.vowl.ui.FlatButtonRating
	        android:id="@+id/navButton"
	        android:layout_width="match_parent"
	        android:layout_height="100dp"
	        android:layout_margin="3dp"
	        android:numStars="7"
		    android:rating="0"
		    android:isIndicator="true" 
	        vowl:baseColor="@color/blueLight"
	        vowl:shadowColor="@color/blueDark"/>
	
	    <edu.cmu.rwsefe.vowl.ui.FlatButtonRating
	        android:id="@+id/navButton2"
	        android:layout_width="match_parent"
	        android:layout_height="match_parent"
	        android:layout_margin="3dp"
	        vowl:baseColor="@color/blueLight"
	        vowl:shadowColor="@color/blueDark"/>
	    
	   <edu.cmu.rwsefe.vowl.ui.FlatButtonRating
	       android:id="@+id/navButton3"
	       android:layout_width="match_parent"
	       android:layout_height="match_parent"
	       android:layout_margin="3dp"
	       vowl:baseColor="@color/blueLight"
	       vowl:shadowColor="@color/blueDark"/>
	    
	</TableRow>

	
	<TableRow
	    android:layout_width="match_parent"
	    android:layout_height="match_parent" >
	    
	    <edu.cmu.rwsefe.vowl.ui.FlatButtonRating
	        android:id="@+id/navButton4"
	        android:layout_width="match_parent"
	        android:layout_height="100dp"
	        android:layout_margin="3dp"
	        vowl:baseColor="@color/blueLight"
	        vowl:shadowColor="@color/blueDark"/>
	
	    <edu.cmu.rwsefe.vowl.ui.FlatButtonRating
	        android:id="@+id/navButton5"
	        android:layout_width="match_parent"
	        android:layout_height="match_parent"
	        android:layout_margin="3dp"
	        vowl:baseColor="@color/blueLight"
	        vowl:shadowColor="@color/blueDark"/>
	    
	    <edu.cmu.rwsefe.vowl.ui.FlatButtonRating
	        android:id="@+id/navButton6"
	        android:layout_width="match_parent"
	        android:layout_height="match_parent"
	        android:layout_margin="3dp"
	        vowl:baseColor="@color/blueLight"
	        vowl:shadowColor="@color/blueDark"/>

	</TableRow>
	
	
	<TableRow
	    android:layout_width="match_parent"
	    android:layout_height="match_parent" >
	    
	    <edu.cmu.rwsefe.vowl.ui.FlatButtonRating
	        android:id="@+id/navButton7"
	        android:layout_width="match_parent"
	        android:layout_height="100dp"
	        android:layout_margin="3dp"
	        vowl:baseColor="@color/blueLight"
	        vowl:shadowColor="@color/blueDark"/>
	
	    <edu.cmu.rwsefe.vowl.ui.FlatButtonRating
	        android:id="@+id/navButton8"
	        android:layout_width="match_parent"
	        android:layout_height="match_parent"
	        android:layout_margin="3dp"
	        vowl:baseColor="@color/blueLight"
	        vowl:shadowColor="@color/blueDark"/>
	    
	    <edu.cmu.rwsefe.vowl.ui.FlatButtonRating
	        android:id="@+id/navButton9"
	        android:layout_width="match_parent"
	        android:layout_height="match_parent"
	        android:layout_margin="3dp"
	        vowl:baseColor="@color/blueLight"
	        vowl:shadowColor="@color/blueDark"/>

	</TableRow>
</TableLayout>